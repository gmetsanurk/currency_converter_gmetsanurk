platform :ios, '14.0'

def common_pods
  pod 'SnapKit', '~> 5.7.1'
  pod 'Moya', '~> 15.0'
  pod 'Moya/Combine', '~> 15.0'
  pod 'NetworkManager', :path => 'NetworkManager'
end

target 'CurrentConverterApp' do
